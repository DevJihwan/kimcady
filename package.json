{
  "name": "kimcaddie",
  "version": "1.0.0",
  "description": "KimCaddie 예약 데이터 수집기",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.KimCaddie",
    "productName": "KimCaddie",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "uninstallDisplayName": "KimCaddie",
      "shortcutName": "KimCaddie"
    }
  },
  "dependencies": {
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "electron": "^30.0.1",
    "puppeteer-core": "^22.6.5"
  },
  "devDependencies": {
    "electron-builder": "^24.13.3"
  }
}